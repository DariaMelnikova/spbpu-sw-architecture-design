<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<?import javafx.scene.control.PasswordField?>
<AnchorPane prefHeight="322.0" prefWidth="311.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StartController">
    <children>
        <Label layoutX="120.0" layoutY="14.0" text="Вешалка">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Button fx:id="addUserButton"
                layoutX="14.0" layoutY="283.0"
                mnemonicParsing="false"
                prefHeight="25.0" prefWidth="283.0"
                text="Добавить пользователя"/>
        <Button fx:id="enterButton"
                layoutX="14.0" layoutY="196.0"
                mnemonicParsing="false"
                prefHeight="25.0" prefWidth="283.0"
                text="Войти"/>
        <TextField fx:id="loginTextField" layoutX="14.0" layoutY="98.0" prefHeight="25.0" prefWidth="283.0"/>
        <PasswordField fx:id="passwordTextField" layoutX="14.0" layoutY="149.0" prefHeight="25.0" prefWidth="283.0"/>
        <Label layoutX="14.0" layoutY="81.0" text="Логин"/>
        <Label layoutX="14.0" layoutY="132.0" text="Пароль"/>
    </children>
</AnchorPane>
